        "@microsoft/applicationinsights-web": "^2.6.5",
        "@peculiar/webcrypto": "^1.1.7",
        "@testing-library/jest-dom": "^5.1.0",
        "@testing-library/react": "^9.5.0",
        "@types/node": "^16.6.1",
        "@types/react": "^17.0.18",
        "@types/react-dom": "^17.0.9",
        "@types/react-router": "^5.1.16",
        "@types/react-router-dom": "^5.1.8",
