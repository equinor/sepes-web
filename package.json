    "dependencies": {
        "@azure/storage-blob": "^12.4.1",
        "@equinor/eds-core-react": "^0.8.5",
        "@fortawesome/fontawesome-free": "^5.15.2",
        "@fortawesome/fontawesome-svg-core": "^1.2.34",
        "@fortawesome/free-solid-svg-icons": "^5.15.2",
        "@fortawesome/react-fontawesome": "^0.1.14",
        "@material-ui/core": "^4.11.3",
        "@microsoft/applicationinsights-web": "^2.5.11",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "@types/jest": "^24.9.1",
        "@types/node": "^12.19.15",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^16.9.10",
        "@types/react-router": "^5.1.11",
        "@types/react-router-dom": "^5.1.7",
        "@types/reactstrap": "^8.7.1",
        "@types/styled-components": "^5.1.7",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.15.0",
        "animate.css": "^4.1.1",
        "axios": "^0.21.1",
        "browser-image-compression": "^1.0.14",
        "commitizen": "^4.2.3",
        "copy-to-clipboard": "^3.3.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "identity-obj-proxy": "^3.0.0",
        "jest-transform-stub": "^2.0.0",
        "js-cookie": "^2.2.1",
        "lodash": ">=4.17.19",
        "lodash.debounce": "^4.0.8",
        "msal": "1.3.0",
        "node-forge": ">=0.10.0",
        "node-sass": "^4.14.1",
        "object-path": ">=0.11.5",
        "react": "^16.13.1",
        "react-copy-to-clipboard": "^5.0.3",
        "react-dom": "^16.13.1",
        "react-dropzone": "^11.3.1",
        "react-notifications-component": "^2.4.1",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^4.0.3",
        "react-select": "^3.2.0",
        "reactstrap": "^8.9.0",
        "serialize-javascript": ">=3.1.0",
        "styled-components": "^5.2.1",
        "ts-jest": "^26.5.4",
        "typescript": "^4.2.3"
    },
    "scripts": {
        "start": "react-scripts -r @cypress/instrument-cra start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "commit": "git-cz",
        "report:combined": "npx nyc report --reporter lcov --reporter text --report-dir coverage",
        "analyze": "node scripts/analyze.js"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@cypress/code-coverage": "^3.7.5",
        "@cypress/instrument-cra": "^1.1.1",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.15.0",
        "cypress": "^4.6.0",
        "cypress-dark": "^1.7.14",
        "cypress-plugin-retries": "^1.5.2",
        "cz-conventional-changelog": "^3.2.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^2.5.0",
        "progress-bar-webpack-plugin": "^2.1.0",
        "webpack-bundle-analyzer": "^3.8.0"
    },
    "jest": {
        "transformIgnorePatterns": [
            "node_modules/(?!(@equinor/eds-icons)/)",
            "cypress"
        ],
        "transform": {
            "^.+\\.tsx?$": "babel-jest",
            "^.+\\.ts?$": "ts-jest",
            ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
        },
        "moduleNameMapper": {
            "\\.(css|less)$": "identity-obj-proxy"
        }
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "nyc": {
        "report-dir": "cypress-coverage"
    },
    "resolutions": {
        "**/@typescript-eslint/eslint-plugin": "^4.1.1",
        "**/@typescript-eslint/parser": "^4.1.1"
    },
    "babel": {
        "presets": [
            "@babel/react",
            "@babel/preset-env",
            "@babel/preset-typescript"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
    }
}
